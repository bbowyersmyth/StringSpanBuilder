      "buildOptions": {
        "define": [ "NOCOMPLETEDTASK" ]
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.IO": "4.1.0",
        "System.Resources.ResourceManager": "4.0.1",
        "System.Runtime": "4.1.0",
        "System.Text.Encoding.Extensions": "4.0.11",
        "System.Threading": "4.0.11"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "NOCOMPLETEDTASK" ]
      }
    },
    "net46": {
    }
